<div
  class="joinYourCommunity">
  <div
    class="container">
    <h2>{{ section.settings.heading }}</h2>
    {{ section.settings.short_code }}
  </div>
</div>

{% schema %}
  {
    "name": "Instagram",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading",
        "default": "Join the community"
      }, {
        "type": "text",
        "id": "short_code",
        "label": "Instafeed Short Code"
      }
    ],
    "presets": [
      {
        "name": "Instagram"
      }
    ]
  }
{% endschema %}