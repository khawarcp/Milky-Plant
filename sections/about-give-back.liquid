<section class="you-sec">
    <div class="container">
        <div class="inner-helo">
            <h3> {{ section.settings.heading }} </h3>
            <p> {{ section.settings.text }} </p>
          
        </div>
        <div class="partner-flex">
            {% for block in section.block %}                                  
                <div class="partner-width">
                    {% render 'responsive-image', image: block.settings.image %}
                    <span> {{ block.settings.heading }} </span>
                </div>                
            {% endfor %}
        </div>
       
    </div>
</section>

{% schema %}
    {
        "name": "Give Back",
        "settings": [
            {
                "type": "text",
                "id": "heading",
                "label": "Heading"
            },
            {
                "type": "inline_richtext",
                "id": "text",
                "label": "Text"
            }
        ],
        "blocks": [
            {
                "name": "Block",
                "type": "block",
                "settings": [
                    {
                        "type": "text",
                        "id": "heading",
                        "label": "Heading"
                    },
                    {
                        "type": "image_picker",
                        "id": "image",
                        "label": "Images"
                    }
                ]
            }
        ]
    }
{% endschema %}
