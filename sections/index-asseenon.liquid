<div class="asseenOn">
  <div class="container">
    <h2>
      {{ section.settings.heading }}
    </h2>
    <div class="sliderContainerAssenOn">
      <div class="slideWrapepr">
        {% for block in section.blocks %}
          {% render 'responsive-image', image: block.settings.image %}
        {% endfor %}
      </div>
    </div>
  </div>
</div>

{% schema %}
  {
    "name": "As Seen On",
    "settings": [
      {
        "type": "text",
        "id": "heading",
        "label": "Heading",
        "default": "As seen on"
      }
    ],
    "blocks": [
      {
        "name": "Block",
        "type": "image",
        "settings": [
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "As Seen On"
      }
    ]
  }
{% endschema %}